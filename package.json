{
  "type": "module",
  "name": "firefox-launch",
  "private": true,
  "version": "1.0.0",
  "description": "Extenision that enables the user to launch Firefox from the other browser",
  "main": "backgroundScripts/background.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:glean && NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" c8 jest",
    "lint": "eslint --fix src test",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "coverage": "c8 report --reporter=lcov --reporter=text",
    "zip:firefox": "web-ext build --source-dir build/firefox --artifacts-dir dist --overwrite-dest --filename=firefox.zip",
    "zip:chromium": "web-ext build --source-dir build/chromium --artifacts-dir dist --overwrite-dest --filename=chromium.zip",
    "build": "npm run build:firefox && npm run build:chromium && npm run zip:firefox && npm run zip:chromium",
    "build:chromium": "npm run build:glean && webpack --node-env=chromium --config webpack.config.mjs",
    "build:firefox": "npm run build:glean && webpack --node-env=firefox --config webpack.config.mjs",
    "lint:chromium": "web-ext lint --source-dir build/chromium",
    "lint:firefox": "web-ext lint --source-dir build/firefox",
    "build:glean": "glean translate metrics.yaml pings.yaml -f javascript -o src/shared/generated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla/firefox-launch.git"
  },
  "keywords": [
    "firefox",
    "addon",
    "chrome",
    "edge"
  ],
  "author": "mozilla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mozilla/firefox-launch/issues"
  },
  "homepage": "https://github.com/mozilla/firefox-launch#readme",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "c8": "^8.0.1",
    "chrome-types": "^0.1.246",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.54.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "python": "^0.0.4",
    "web-ext": "^7.9.0",
    "webextensions-jsdom": "^1.4.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@mozilla/glean": "^2.0.0"
  }
}
