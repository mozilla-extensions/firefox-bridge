    "chai": "^4.3.10",
    "chrome-types": "^0.1.246",
    "eslint": "^8.54.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "webextensions-jsdom": "^1.4.1"
